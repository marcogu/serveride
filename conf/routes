# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.MainController.appIdx
GET		/card/:info					controllers.SoftEnryptController.parserCardInfo(info)
GET		/card/:info/:tslog/:mcModel controllers.SoftEnryptController.decodeTranc2By(info, tslog, mcModel)
GET		/wkey/:r/:ts/:ksn			controllers.SoftEnryptController.wkey(r, ts, ksn)
GET		/mac/:wk/:data				controllers.SoftEnryptController.macCacWith(wk,data)
GET		/3des/:key/:data			controllers.SoftEnryptController.des3(key,data)
POST    /mockserivce/:macModel      controllers.SoftEnryptController.cqjrMockDeal(macModel)

# Map static resources from the /public folder to the /assets URL path
GET     /AngularJsFramework/app/*file   controllers.Assets.at(path="/public/app", file)
GET     /assets/javascripts/*file       controllers.Assets.at(path="/public/javascripts", file)
GET     /assets/stylesheets/*file       controllers.Assets.at(path="/public/stylesheets", file)
GET     /assets/images/*file            controllers.Assets.at(path="/public/images", file)

    